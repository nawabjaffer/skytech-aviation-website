# Google Sheets Configuration
# Copy this file to .env and fill in your values

# Google Sheet ID (from the URL: https://docs.google.com/spreadsheets/d/{SHEET_ID}/edit)
VITE_GOOGLE_SHEET_ID=

# Google API Key (optional, required only for private sheets)
VITE_GOOGLE_API_KEY=

# Sheet range (default: Sheet1!A2:L100)
VITE_GOOGLE_SHEET_RANGE=Sheet1!A2:L100

# Webhook URL for real-time updates (optional)
VITE_GOOGLE_WEBHOOK_URL=
